function GetQuest()
	CheckQuest()
	local args = {
		[1] = "StartQuest",
		[2] = NameQ,
		[3] = LvQ
	}

	game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
end

local function teleportWithCustomSpeed(character: Model, targetPosition: Vector3)
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	-- ‡∏ó‡∏≥‡πÉ‡∏´‡πâ‡∏ó‡∏∞‡∏•‡∏∏
	for _, part in ipairs(character:GetDescendants()) do
		if part:IsA("BasePart") then
			part.CanCollide = false
		end
	end

	-- ‡∏™‡∏£‡πâ‡∏≤‡∏á platform
	local platform = Instance.new("Part")
	platform.Size = Vector3.new(6, 1, 6)
	platform.Anchored = true
	platform.CanCollide = true
	platform.Transparency = 0.3
	platform.Material = Enum.Material.Neon
	platform.Color = Color3.fromRGB(0, 255, 128)
	platform.Name = "TweenPlatform"
	platform.Parent = workspace

	-- ‡πÉ‡∏´‡πâ platform ‡∏ï‡∏≤‡∏°‡∏ï‡∏¥‡∏î‡∏ï‡∏±‡∏ß
	local followConn
	followConn = RunService.Heartbeat:Connect(function()
		if hrp and platform then
			platform.Position = hrp.Position - Vector3.new(0, hrp.Size.Y/2 + 1.5, 0) + Vector3.new(0, -1.5, 0)
		else
			followConn:Disconnect()
		end
	end)

	-- ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà‡πÄ‡∏≠‡∏á + ‡πÄ‡∏£‡πà‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÉ‡∏Å‡∏•‡πâ‡∏ñ‡∏∂‡∏á
	local minSpeed = 300
	local maxSpeed = 500
	local accelerationDistance = 350
	local threshold = 1

	local moveConn
	moveConn = RunService.Heartbeat:Connect(function(dt)
		if not hrp then moveConn:Disconnect() return end

		local currentPos = hrp.Position
		local direction = targetPosition - currentPos
		local distance = direction.Magnitude

		if distance < threshold then
			hrp.Position = targetPosition
			if platform then platform:Destroy() end
			moveConn:Disconnect()
			return
		end

		local t = 1 - math.clamp(distance / accelerationDistance, 0, 1)
		local speed = minSpeed + (maxSpeed - minSpeed) * t

		local moveVec = direction.Unit * speed * dt
		if moveVec.Magnitude > distance then
			moveVec = direction
		end

		hrp.CFrame = hrp.CFrame + moveVec
	end)
end






local RunService = game:GetService("RunService")

function BringAndFreezeMultipleMobs(PosMon, NameM)
    local enemies = workspace:FindFirstChild("Enemies")
    if not enemies then return end

    for _, mob in ipairs(enemies:GetChildren()) do
        if mob:IsA("Model") and mob.Name == NameM then
            local hrp = mob:FindFirstChild("HumanoidRootPart")
            local hum = mob:FindFirstChildOfClass("Humanoid")

            if hrp and hum and hum.Health > 0 then
                -- ‡∏•‡∏ö BodyMover ‡πÅ‡∏õ‡∏•‡∏Å‡∏õ‡∏•‡∏≠‡∏°‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î
                for _, v in ipairs(hrp:GetChildren()) do
                    if v:IsA("BodyMover") or v:IsA("BodyPosition") or v:IsA("BodyGyro") then
                        v:Destroy()
                    end
                end

                -- ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡∏ü‡∏¥‡∏™‡∏¥‡∏Å‡∏™‡πå‡∏û‡∏∑‡πâ‡∏ô‡∏ê‡∏≤‡∏ô
                hrp.Anchored = false
                hrp.CanCollide = false
                hrp.AssemblyLinearVelocity = Vector3.zero
                hrp.AssemblyAngularVelocity = Vector3.zero

                -- ‡∏õ‡∏¥‡∏î‡∏Å‡∏≤‡∏£‡∏ä‡∏ô‡∏ó‡∏∏‡∏Å‡∏ä‡∏¥‡πâ‡∏ô‡∏™‡πà‡∏ß‡∏ô ‡πÅ‡∏ï‡πà‡∏¢‡∏±‡∏á‡∏Ñ‡∏á‡πÅ‡∏™‡∏î‡∏á‡∏ú‡∏•
                for _, part in ipairs(mob:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.Anchored = false
                        part.CanCollide = false
                        part.Transparency = 0
                    end
                end

                -- ‡∏™‡∏£‡πâ‡∏≤‡∏á BodyMover ‡∏Ç‡∏≠‡∏á‡πÄ‡∏£‡∏≤
                local bp = Instance.new("BodyPosition")
                bp.Name = "BringBP"
                bp.MaxForce = Vector3.new(1e9, 1e9, 1e9)
                bp.D = 1000
                bp.P = 20000
                bp.Position = PosMon.Position
                bp.Parent = hrp

                local bg = Instance.new("BodyGyro")
                bg.Name = "FreezeGyro"
                bg.MaxTorque = Vector3.new(1e9, 1e9, 1e9)
                bg.P = 1e5
                bg.CFrame = CFrame.new(PosMon.Position)
                bg.Parent = hrp

                -- ‡πÉ‡∏ä‡πâ RenderStepped ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏•‡πá‡∏≠‡∏Å‡πÅ‡∏•‡∏∞‡∏Ñ‡∏ß‡∏ö‡∏Ñ‡∏∏‡∏°‡∏≠‡∏¢‡πà‡∏≤‡∏á‡∏£‡∏≤‡∏ö‡∏£‡∏∑‡πà‡∏ô
                local conn
                conn = RunService.RenderStepped:Connect(function()
                    if not mob or not mob.Parent or hum.Health <= 0 then
                        if bp and bp.Parent then bp:Destroy() end
                        if bg and bg.Parent then bg:Destroy() end
                        if conn then conn:Disconnect() end
                        return
                    end

                    -- ‡∏•‡∏ö BodyMover ‡πÅ‡∏õ‡∏•‡∏Å‡∏õ‡∏•‡∏≠‡∏°‡∏ó‡∏µ‡πà‡∏≠‡∏≤‡∏à‡∏ñ‡∏π‡∏Å‡πÅ‡∏ó‡∏£‡∏Å
                    for _, v in ipairs(hrp:GetChildren()) do
                        if (v:IsA("BodyMover") or v:IsA("BodyPosition") or v:IsA("BodyGyro")) and not (v.Name == "BringBP" or v.Name == "FreezeGyro") then
                            v:Destroy()
                        end
                    end

                    -- ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ï‡∏≥‡πÅ‡∏´‡∏ô‡πà‡∏á‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ (‡∏´‡∏≤‡∏Å PosMon ‡πÄ‡∏Ñ‡∏•‡∏∑‡πà‡∏≠‡∏ô‡∏ó‡∏µ‡πà)
                    bp.Position = PosMon.Position
                    bg.CFrame = CFrame.new(PosMon.Position)

                    -- ‡∏´‡∏¢‡∏∏‡∏î‡∏î‡∏∂‡∏á‡πÄ‡∏°‡∏∑‡πà‡∏≠‡πÄ‡∏Ç‡πâ‡∏≤‡πÉ‡∏Å‡∏•‡πâ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
                    if (hrp.Position - PosMon.Position).Magnitude < 1 then
                        if bp and bp.Parent then bp:Destroy() end
                    end
                end)
            end
        end
    end
end















function FastAttack()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local root = character:WaitForChild("HumanoidRootPart")
    local enemiesFolder = workspace:FindFirstChild("Enemies")

    if not enemiesFolder then return end

    pcall(function()
        ReplicatedStorage:WaitForChild("Modules"):WaitForChild("Net"):WaitForChild("RE/RegisterAttack"):FireServer(0.5)
    end)

    for _, enemy in ipairs(enemiesFolder:GetChildren()) do
        if enemy:IsA("Model") and enemy:FindFirstChild("Head") and enemy:FindFirstChild("HumanoidRootPart") then
            local humanoid = enemy:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health > 0 then
                local dist = (enemy.Head.Position - root.Position).Magnitude
                if dist <= 60 then
                    -- ‡∏ï‡∏µ‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡∏´‡∏•‡∏≤‡∏¢‡∏ï‡∏±‡∏ß
                    pcall(function()
                        ReplicatedStorage:WaitForChild("Modules"):WaitForChild("Net"):WaitForChild("RE/RegisterHit"):FireServer(enemy.Head)
                    end)
                    task.wait(0.05) -- ‡∏´‡∏ô‡πà‡∏ß‡∏á‡πÄ‡∏ß‡∏•‡∏≤‡∏ô‡∏¥‡∏î‡∏´‡∏ô‡πà‡∏≠‡∏¢‡πÄ‡∏û‡∏∑‡πà‡∏≠‡πÉ‡∏´‡πâ‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡πÄ‡∏ß‡∏≠‡∏£‡πå‡∏£‡∏±‡∏ö‡∏ó‡∏±‡∏ô
                end
            end
        end
    end
end







function topos(Pos)
	local hrp = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
		for i, v in pairs(game.Workspace:GetChildren()) do
              if v.Name == "TweenPlatform" then
				v:Destroy()
			  end
		end

	Distance = (Pos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
	if game.Players.LocalPlayer.Character.Humanoid.Sit == true then game.Players.LocalPlayer.Character.Humanoid.Sit = false end
	pcall(function() tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart,TweenInfo.new(Distance/300, Enum.EasingStyle.Linear),{CFrame = Pos}) end)
	tween:Play()
	if Distance <= 250 then
		tween:Cancel()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Pos
	end
	if _G.StopTween == true then
		tween:Cancel()
		_G.Clip = false
	end

	tween.Completed:Connect(function()

	end)
end

function GetDistance(target)
	return math.floor((target.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
end


function StopTween(target)
if not target then
	_G.StopTween = true
	wait()
	topos(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
	wait()
	if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip") then
		game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyClip"):Destroy()
	end
	_G.StopTween = false
	_G.Clip = false
end
end

