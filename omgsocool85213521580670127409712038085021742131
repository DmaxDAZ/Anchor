	local Toggle1 = Tabs.Farm:AddToggle("MyToggle", {Title = "Farm Chests (Wip)", Default = false })

	Toggle1:OnChanged(function(tg)
		task.spawn(function()
			while Toggle1.Value do
				-- ‡∏î‡∏∂‡∏á‡∏´‡∏µ‡∏ö‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î‡∏ó‡∏µ‡πà‡∏°‡∏µ‡πÉ‡∏ô‡πÇ‡∏•‡∏Å ‡∏ì ‡∏ï‡∏≠‡∏ô‡∏ô‡∏±‡πâ‡∏ô
				local chests = game.Workspace.ChestModels:GetChildren()
	
				-- ‡∏ñ‡πâ‡∏≤‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏´‡∏µ‡∏ö‡πÄ‡∏•‡∏¢ ‡∏Å‡πá‡∏£‡∏≠‡πÅ‡∏õ‡πä‡∏ö
				if #chests == 0 then
					wait(0.5)
				else
					for _, chest in ipairs(chests) do
						if not Toggle1.Value then return end -- ‡∏ñ‡πâ‡∏≤ toggle ‡∏õ‡∏¥‡∏î ‡πÉ‡∏´‡πâ‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
	
						if chest and chest:FindFirstChild("RootPart") then
							-- ‡πÄ‡∏ó‡πÄ‡∏•‡∏û‡∏≠‡∏£‡πå‡∏ï‡πÑ‡∏õ‡∏ó‡∏µ‡πà‡∏´‡∏µ‡∏ö‡∏ô‡∏±‡πâ‡∏ô
							teleportWithCustomSpeed(char, chest.RootPart.CFrame.Position)
	
							-- ‡∏£‡∏≠‡∏à‡∏ô‡∏Å‡∏ß‡πà‡∏≤‡∏´‡∏µ‡∏ö‡∏ô‡∏±‡πâ‡∏ô‡∏à‡∏∞‡∏ñ‡∏π‡∏Å‡πÄ‡∏Å‡πá‡∏ö (‡∏´‡∏≤‡∏¢‡πÑ‡∏õ)
							repeat
								wait(0.1)
							until not Toggle1.Value or not chest:IsDescendantOf(game.Workspace.ChestModels)
	
							if not Toggle1.Value then return end -- ‡∏ñ‡πâ‡∏≤‡∏£‡∏∞‡∏´‡∏ß‡πà‡∏≤‡∏á‡∏£‡∏≠‡∏Å‡∏î‡∏õ‡∏¥‡∏î toggle ‡∏Å‡πá‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏±‡∏ô‡∏ó‡∏µ
						end
					end
				end
				wait(0.1) -- ‡∏£‡∏≠‡πÅ‡∏õ‡πä‡∏ö‡∏Å‡πà‡∏≠‡∏ô‡πÄ‡∏ä‡πá‡∏Å‡∏´‡∏µ‡∏ö‡∏£‡∏≠‡∏ö‡πÉ‡∏´‡∏°‡πà (‡πÄ‡∏ú‡∏∑‡πà‡∏≠‡∏°‡∏µ spawn ‡πÉ‡∏´‡∏°‡πà)
			end
		end)
	end)
	

	local Toggle2 = Tabs.Main:AddToggle("MyToggle", {Title = "Fast Attack", Default = false })

	Toggle2:OnChanged(function(tg)
		task.spawn(function()
			while Toggle2.Value do
                task.wait()
                FastAttack()
			end
		end)
	end)
	
